package com.scp.businesslogic;

import static io.restassured.RestAssured.*;
import io.restassured.RestAssured;

class D1 {

	public static void main(String[] args) {

		RestAssured.baseURI="https://reqres.in";
	
		RestAssured.baseURI = "https://reqres.in";
		given().
		param("page","2");
		
		when().
		get("/api/users");
		
	    then().
	           assertThat().statusCode(200);
		
				
	}


}
