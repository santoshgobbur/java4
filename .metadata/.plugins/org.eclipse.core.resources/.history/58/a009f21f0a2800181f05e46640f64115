public class NewTest1{

	WebDriver driver;
	 @Test
	 @Parameters("url")
	 public void Login(String url){
		 
		 System.setProperty("webdriver.gecko.driver","Resource/geckodriver.exe");
		 driver.get(url);
		 
	 }

	
}
